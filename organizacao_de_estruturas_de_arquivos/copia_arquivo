#include<stdio.h>

int main(int argc, char* argv[]) {
    FILE* file_stream, copia;
    char caractere;
    file_stream = fopen("/home/mathe/Faculdade/Disciplinas/Arquivos/saida.txt", "rb");
    copia = fopen("copia_saida.txt", "w"); //cria o arquivo caso ele já não exista
    while((caractere = fgetc()) != EOF) {
        fwrite(caractere, sizeof(char), 1, copia);
    }
    fclose(file_stream);
    fclose(copia);
    return 0; //retorna para o código do kernel dando a entender que ocorreu tudo como previsto dentro do programa!
}